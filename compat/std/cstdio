#ifndef IGRIS_STD_CSTDIO_H
#define IGRIS_STD_CSTDIO_H

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

namespace std
{
    using FILE = ::FILE;
    using fpos_t = ::fpos_t;
    using size_t = ::size_t;
    using off_t = ::off_t;
    using ssize_t = ::ssize_t;

    using ::sprintf;
}

#endif
